<!DOCTYPE html>
<html>
	<head>
	  <title>Project</title>
	  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
	  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
	  <%= csrf_meta_tags %>
	  <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

	</head>
	<body>
		<p class="test">
		<% if user_signed_in? %>
			  Logged in as <strong><%= current_user.email %></strong>.
			  <%= link_to 'Edit profile', edit_user_registration_path, :class => 'test' %> |
			  <%= link_to "Logout", destroy_user_session_path, method: :delete, :class => 'test'  %>
		<% else %>
			  <%= link_to "Sign up", new_user_registration_path, :class => 'test'  %> |
			  <%= link_to "Login", new_user_session_path, :class => 'test'  %>
		<% end %>

		<% if notice %>
		  <p class="alert alert-success"><%= notice %></p>
		<% end %>
		<% if alert %>
		  <p class="alert alert-danger"><%= alert %></p>
		<% end %>
		<%= yield %>
	</body>
</html>

